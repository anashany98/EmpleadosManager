{
    "name": "nominas-backend",
    "version": "1.0.0",
    "description": "Backend API for NominasApp",
    "main": "src/index.ts",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "nodemon src/index.ts",
        "build": "tsc",
        "prisma:generate": "prisma generate --schema=../database/prisma/schema.prisma",
        "prisma:migrate": "prisma migrate dev --schema=../database/prisma/schema.prisma",
        "seed:admin": "ts-node src/scripts/seed-admin.ts",
        "seed:profiles": "ts-node src/scripts/seed-profiles.ts",
        "seed:data": "ts-node src/scripts/populate_data.ts"
    },
    "dependencies": {
        "@prisma/client": "^5.x",
        "@types/pdfkit": "^0.17.4",
        "@types/qrcode": "^1.5.6",
        "bcryptjs": "^3.0.3",
        "canvas": "^3.2.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "exceljs": "^4.4.0",
        "express": "^4.18.2",
        "imapflow": "^1.2.4",
        "jpeg-js": "^0.4.4",
        "jsonwebtoken": "^9.0.3",
        "jsqr": "^1.4.0",
        "mailparser": "^3.9.1",
        "multer": "^1.4.5-lts.1",
        "pdf-lib": "^1.17.1",
        "pdfjs-dist": "^5.4.530",
        "pdfkit": "^0.17.2",
        "pngjs": "^7.0.0",
        "qrcode": "^1.5.4",
        "tesseract.js": "^7.0.0",
        "xlsx": "^0.18.5",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/mailparser": "^3.4.6",
        "@types/multer": "^1.4.13",
        "@types/node": "^20.10.4",
        "@types/uuid": "^10.0.0",
        "nodemon": "^3.0.2",
        "prisma": "^5.x",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    }
}