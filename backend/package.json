        "express": "^4.18.2",
        "multer": "^1.4.5-lts.1",
        "tesseract.js": "^7.0.0",
        "xlsx": "^0.18.5",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/multer": "^1.4.13",
        "@types/node": "^20.10.4",
        "nodemon": "^3.0.2",
        "prisma": "^5.x",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    }
